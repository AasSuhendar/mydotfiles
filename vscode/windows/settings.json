{
    "terminal.integrated.confirmOnExit": true,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/aassuhendar/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.minikube-path": "/Users/aassuhendar/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.kubectl-path": "/Users/aassuhendar/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.draft-path": "/Users/aassuhendar/.vs-kubernetes/tools/draft/darwin-amd64/draft"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.tabWidth": 4,
    "workbench.iconTheme": "vscode-icons",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "go.testFlags": [
        "-v"
    ],
    "go.testEnvFile": "",
    "go.inferGopath": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "redhat.telemetry.enabled": false,
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "yaml.customTags": [
        "!encrypted/pkcs1-oaep scalar",
        "!vault scalar"
    ],
    "files.autoSave": "afterDelay",
    "files.associations": {
        "*.yml": "ansible"
    },
    "terminal.integrated.fontSize": 13,
    "editor.fontFamily": "Menlo",
    "settingsSync.ignoredSettings": [
        "editor.fontFamily",
        "terminal.integrated.fontFamily"
    ],
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-a>": false,
        "<C-s>": false
    },
    "vim.insertModeKeyBindings": [
        {
          "before": ["j", "j"],
          "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
          "before": ["v"],
          "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
          "before": ["<leader>", "d"],
          "after": ["d", "d"]
        },
        {
          "before": ["<C-n>"],
          "commands": [":nohl"]
        }
    ],
}